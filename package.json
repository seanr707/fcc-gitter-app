{
  "name": "fcc-gitter-app",
  "version": "0.0.1",
  "description": "Display FreeCodeCamp Gitter channels",
  "main": "index.js",
  "scripts": {
    "start": "npm install && npm run build && node dist/server",
    "start-dev": "babel-node dist/server",
    "clean": "rm -r dist/server && rm dist/client/css/main.css && dist/client/js/bundle.js",
    "build-front": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "build-front-dev": "webpack --progress --colors",
    "build-server": "babel src/server -d dist/server",
    "build": "npm run build-front && npm run build-server",
    "build-dev": "npm run build-front-dev && npm run build-server",
    "deploy": "git checkout heroku && git merge --no-commit --no-ff development && npm run build && git commit -a -m \"New build for Heroku\" && git push heroku heroku:master && git checkout development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "freecodecamp",
    "gitter"
  ],
  "engines": {
    "node": "6.3.1",
    "npm": "3.10.7"
  },
  "author": "seanr707",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "axios": "^0.14.0",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.1.0",
    "markdown-it-emoji": "^1.3.0",
    "mongoose": "^4.6.1",
    "node-gitter": "^2.0.5",
    "passport": "^0.3.2",
    "passport-gitter": "^0.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-mentions": "^0.6.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.10.0",
    "nodemon": "^1.11.0",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-loader": "^0.13.0",
    "postcss-selector-not": "^2.0.0",
    "precss": "^1.4.0",
    "sass": "^0.5.0",
    "sass-loader": "^4.0.2",
    "webpack": "^1.13.2"
  }
}
